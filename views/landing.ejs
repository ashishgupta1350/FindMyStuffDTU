<% include partials/header %>
    <!--The div element for the map -->
    <div id="map"></div>

    <div class="index-buttons">
        <div class="container container-buttons">
            <a href="/items/new"><button class="btn btn-danger">I have lost something!</button></a>
            <a href="/items/new"><button class="btn btn-success">I have found something!</button></a>
        </div>
    </div>

    <script>
        // Initialize and add the map
        function initMap() {
        // The location of DTU
        var dtu = {lat: 28.7500749, lng: 77.1176652};
        // The map, centered at DTU
        var map = new google.maps.Map(
            document.getElementById('map'), {zoom: 17, center: dtu});
        // The marker, positioned at DTU
        var marker = new google.maps.Marker({position: dtu, map: map});
        }
    </script>
  
    <%
        src="https://maps.googleapis.com/maps/api/js?key=".concat(process.env.GEOCODER_API_KEY_MAIN).concat('&callback=initMap');
        console.log(src);
    %>
    <script async defer src=<%="https://maps.googleapis.com/maps/api/js?key=".concat(process.env.GEOCODER_API_KEY_MAIN).concat('&callback=initMap')%>
    </script>
